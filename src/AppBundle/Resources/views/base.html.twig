<!DOCTYPE html>
<html>
<head><!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <!-- Optional theme -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap-theme.min.css">
    <!-- GoogleJs -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <meta charset="UTF-8" />

    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/app/css/sidebar.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/mainContent.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/app/css/myTables.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>


<!-- Wrapper Begin -->
<div id="wrapper">

    <!-- NAVIGATION TOP BAR -->
    <div class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#top-nav-bar">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="{{ path('homepage') }}" class="navbar-brand">Insatech Equipment</a>
            </div>

            <div class="collapse navbar-collapse" id="top-nav-bar">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="{{ path('fos_user_profile_show') }}">Profile</a> </li>
                    <li><a href="{{ path('fos_user_security_logout') }}">Logout</a> </li>
                </ul>
            </div>
        </div><!-- Container End -->
    </div><!-- Navbar end -->




</div> <!-- Wrapper End -->


{% block body %}{% endblock %}
{% block javascripts %}{% endblock %}

<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
</body>
</html>




