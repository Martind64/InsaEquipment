{% extends '@App/layoutBase.html.twig' %}


{% block index %}

        <!-- Column Begin -->
        <div class="col-lg-2">
            <!-- Table Begin -->
            <table class="table table-hover" id="myTables">
                <tc><h1>Prefix</h1></tc>
                <th>
                    <tr>
                        <th>Prefix</th>
                    </tr>
                </th>
                {% for p in prefix %}
                    <tr onclick="document.location = '{{ path('updatePrefix', {'id': p.id}) }}'">
                        <td>{{ p.prefix }}</td>
                    </tr>
                {% endfor %}
            </table><!-- Table End -->
        </div><!-- Column End -->

{% endblock %}
