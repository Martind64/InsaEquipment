{% extends 'AppBundle::sidebar.html.twig'%}

{% block body %}

    <!-- Wrapper begin -->
    <div class="container-fluid" id="main-content-wrapper">
        <div class="container-fluid"><!-- Container Begin -->
            <div class="row"><!-- Row Begin -->
                <!-- Shows the sidebar -->
                {% block sidebar %}{{ parent() }}{% endblock %}

                <!-- Column Begin -->
                <div class="col-lg-8">
                    <h1>Update prefix</h1>
                </div><!-- Column End -->

                <!-- Column Begin -->
                <div class="col-lg-4">
                    <hr>
                    {{ form_start(Form) }}
                    {{ form_widget(Form) }}
                    {{ form_end(Form) }}
                </div><!-- Column End -->

            </div><!-- Row End -->
        </div><!-- Container End -->
    </div><!-- Wrapper End -->


{% endblock %}