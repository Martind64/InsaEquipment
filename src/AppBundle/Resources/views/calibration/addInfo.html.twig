{% extends 'AppBundle::sidebar.html.twig' %}


{% block body %}

    <!-- Wrapper Begin -->
    <div class="container-fluid" id="main-content-wrapper">
        <div class="container-fluid"> <!-- Container Begin -->
            <div class="row"> <!-- Row Begin -->
                <!-- Shows the sidebar -->
                {% block sidebar %}{{ parent() }}{% endblock %}

                <!-- Column Begin -->
                <div class="col-lg-4">
                    {{ form_start(Form) }}
                    {{ form_widget(Form) }}
                    {{ form_end(Form) }}
                    <a href="{{ path('showCalibration', {'id': calibration.id}) }}"><button type="button" class="btn btn-lg btn-primary">Show calibration</button></a>
                </div><!-- Column End -->
            </div><!-- Row End -->
        </div> <!-- Container End -->
    </div><!-- Wrapper End -->

{% endblock %}

