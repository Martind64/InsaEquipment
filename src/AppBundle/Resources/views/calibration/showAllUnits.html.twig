{% extends '@App/layoutBase.html.twig' %}


{% block index %}

    <!-- Column Begin -->
    <div class="col-lg-1">

        <!-- Table Begin -->
        <table class="table table-hover" id="myTables">
            <tc><h1>Units</h1></tc>
            <th>
                <tr>
                    <th>Units</th>
                </tr>
            </th>
            {% for u in units %}
                <tr onclick="document.location = '{{ path('updateUnit', {'id': u.id}) }}'">
                    <td>{{ u.unit }}</td>
                </tr>
            {% endfor %}
        </table><!-- Table End -->
    </div><!-- Column End -->

{% endblock %}
