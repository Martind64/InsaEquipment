{% extends 'AppBundle::sidebar.html.twig'%}



{% block body %}

    <!-- Wrapper begin -->
    <div class="container-fluid" id="main-content-wrapper">
        <div class="container-fluid"><!-- Container Begin -->
            <div class="row"><!-- Row Begin -->
                <!-- Shows the sidebar -->
                {% block sidebar %}{{ parent() }}{% endblock %}

                
                <!-- Run the form -->
                <div class="col-lg-4"><!-- Column Begin -->
                    {{ form_start(Form)}}
                    {{ form_widget(Form)}}
                    {{ form_end(Form)}}
                </div><!-- Column End -->

            </div><!-- Row End -->
        </div><!-- Container End -->
    </div><!-- Wrapper End -->


{% endblock %}